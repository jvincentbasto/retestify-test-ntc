{
  "name": "@retestify/test-ntc",
  "description": "Test Package - Node, Tsup, Changeset (NTC)",
  "version": "0.0.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "cinit": "pnpm changeset init",
    "cpex": "pnpm changeset pre exit",
    "cpen": "pnpm changeset pre enter ${t:=dev}",
    "cver": "pnpm changeset version",
    "scver": "pnpm changeset && pnpm changeset version",
    "lint": "tsc",
    "build": "tsup-node",
    "release": "pnpm build && changeset publish",
    "rc": "[ -d .changeset ] && find .changeset -type f -name '*.md' -delete",
    "rlocbranch": "git branch | grep -v 'master' | xargs git branch -D",
    "rrembranch": "git branch -r | grep -v 'master' | sed 's/origin\\///' | xargs -I {} git push origin --delete {}",
    "rloctag": "git tag | xargs git tag -d",
    "rremtag": "git tag | xargs -I {} git push origin :refs/tags/{}",
    "alias": "echo 'add(a) | remove(r) | set(s) | changesets(c) | local(loc) | remote(rem)'"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.8",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jvincentbasto/retestify-test-ntc.git"
  },
  "keywords": [
    "node",
    "typescript",
    "tsup",
    "changesets"
  ],
  "author": "retestify",
  "bugs": {
    "url": "https://github.com/jvincentbasto/retestify-test-ntc/issues"
  },
  "homepage": "https://github.com/jvincentbasto/retestify-test-ntc#readme"
}
